{
    "$schema": "https://railway.com/railway.schema.json",
    "build": {
      "builder": "RAILPACK",
      "buildCommand": "cd backend && npm install && cd ../frontend && npm install",
      "buildEnvironment": "V3"
    },
    "deploy": {
      "runtime": "V2",
      "numReplicas": 1,
      "startCommand": "npm run dev",
      "sleepApplication": false,
      "useLegacyStacker": false,
      "multiRegionConfig": {
        "us-east4-eqdc4a": {
          "numReplicas": 1
        }
      },
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10
    }
  }